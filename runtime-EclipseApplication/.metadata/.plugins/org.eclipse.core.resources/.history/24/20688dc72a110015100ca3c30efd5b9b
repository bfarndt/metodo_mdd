<?php
	

	class Daniel
	{
		private $id;
		private $cpf;
		private $endereco;
		

		public function __construct()
		{
			$this->cpf = "";
			$this->endereco = "";
			
		}

		public function getId()
		{
			return $this->id;
		}

		public function setId($id)
		{
			$this->id = $id;
		}

	
		public function get()
		{
			return $this->cpf;
		}

		public function set($cpf)
		{
			$this->cpf = $cpf;
		}
	
		public function get()
		{
			return $this->endereco;
		}

		public function set($endereco)
		{
			$this->endereco = $endereco;
		}
	
	}

?>