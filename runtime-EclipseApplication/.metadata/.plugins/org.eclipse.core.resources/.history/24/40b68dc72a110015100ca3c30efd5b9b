<?php
	include_once "Daniel.class.php";

	class Bruno
	{
		private $id;
		private $cpf;
		private $endereco;
		private $nascimento;
		private $mensalidade;
		private $orientador;
		

		public function __construct()
		{
			$this->cpf = "";
			$this->endereco = "";
			$this->nascimento = "";
			$this->mensalidade = "";
			$this->orientador = new Daniel();
			
		}

		public function getId()
		{
			return $this->id;
		}

		public function setId($id)
		{
			$this->id = $id;
		}

	
		public function get()
		{
			return $this->cpf;
		}

		public function set($cpf)
		{
			$this->cpf = $cpf;
		}
	
		public function get()
		{
			return $this->endereco;
		}

		public function set($endereco)
		{
			$this->endereco = $endereco;
		}
	
		public function get()
		{
			return $this->nascimento;
		}

		public function set($nascimento)
		{
			$this->nascimento = $nascimento;
		}
	
		public function get()
		{
			return $this->mensalidade;
		}

		public function set($mensalidade)
		{
			$this->mensalidade = $mensalidade;
		}
	
		public function get()
		{
			return $this->orientador;
		}

		public function set($orientador)
		{
			$this->orientador = $orientador;
		}
	
	}

?>