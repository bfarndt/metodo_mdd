
CREATE TABLE daniel
(
	id bigserial,
	cpf textendereco text,
	CONSTRAINT daniel_pkey PRIMARY KEY (id)
	
);

CREATE TABLE bruno
(
	id bigserial,
	cpf textendereco textnascimento datemensalidade moneyorientador bigint,
	CONSTRAINT bruno_pkey PRIMARY KEY (id)
	CONSTRAINT bruno_orientador_fkey FOREIGN KEY (orientador)
			REFERENCES daniel (id) MATCH SIMPLE
			ON UPDATE NO ACTION ON DELETE NO ACTION
	
);
