<?php
	

	class Daniel
	{
		private $id;
		private $cpf;
		private $endereco;
		

		public function __construct()
		{
			$this->cpf = "";
			$this->endereco = "";
			
		}

		public function getId()
		{
			return $this->id;
		}

		public function setId($id)
		{
			$this->id = $id;
		}

	
		public function getCpf()
		{
			return $this->cpf;
		}

		public function setCpf($cpf)
		{
			$this->cpf = $cpf;
		}
	
		public function getCpf()
		{
			return $this->cpf;
		}
		
		public function addCpf($cpf)
		{
			$this->cpf[] = $cpf;
		}
		
		public function removeCpf($cpf)
		{
			
		}
		
		public function removeAllCpf()
		{
			$this->cpf = array();
		}
	
		public function getEndereco()
		{
			return $this->endereco;
		}

		public function setEndereco($endereco)
		{
			$this->endereco = $endereco;
		}
	
		public function getEndereco()
		{
			return $this->endereco;
		}
		
		public function addEndereco($endereco)
		{
			$this->endereco[] = $endereco;
		}
		
		public function removeEndereco($endereco)
		{
			
		}
		
		public function removeAllEndereco()
		{
			$this->endereco = array();
		}
	
	}

?>