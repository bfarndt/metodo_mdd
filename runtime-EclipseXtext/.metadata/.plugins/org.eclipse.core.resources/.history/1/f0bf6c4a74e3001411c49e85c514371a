package my.company.blog {
	import java.lang.*
	
	entity Blog {
		title: String
		many posts: Post
	}
	
	entity HasAuthor {
		author: String
	}
	
	entity Post extends HasAuthor {
		title: String
		content: String
		many comments: Comment
	}
	
	entity Comment extends HasAuthor {
		content: String
	}
	
}